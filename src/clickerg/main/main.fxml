      </Button>
      <Button fx:id="bt_gacha" layoutX="494.0" layoutY="34.0" mnemonicParsing="false" onAction="#clickGacha" prefHeight="25.0" prefWidth="78.0" text="Gacha" textOverrun="CENTER_ELLIPSIS">
